{%- if section.blocks.size > 0 -%}
<ul id="siteNav" class="siteNavigation {{ section.settings.menuAlign}}{% if settings.nav_below_logo %}{% unless settings.main_header_fullwidth %} page-width{% endunless %}{% endif %}" role="navigation" data-section-id="site-{{ section.id }}" data-section-type="site-navigation">
    {% for block in section.blocks %}
    {%- assign label = block.settings.title | split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
    {%- capture linkLevel1 -%}<a href="{% if block.settings.url != blank %}{{ block.settings.url }}{% else %}#{% endif %}" {% if block.settings.newtab %} target="_blank"{% endif %}>{{ block.settings.title | split: '[label' | first }}{% if section.settings.menuarrow %} <i class="at at-angle-down-l"></i>{% endif %}{% if block.settings.title contains '[label' %}<span class="lbl {{label}}">{{ section.settings[label] }}</span>{% endif %}</a>{%- endcapture -%}
    {%- case block.type -%}
    {%- when 'homelink' -%}
    <li class="lvl1"><a href="{{routes.root_url}}">{{ block.settings.title}}</a></li>

    {%- when 'loginlink' -%}
    {% if customer %}
    <li class="lvl1"><a href="{{ routes.account_url }}">{{ 'customer.account.title' | t }}</a></li>
    <li class="lvl1"><a href="{{ routes.account_logout_url }}">{{ 'customer.account.logout' | t }}</a></li>    
    {% else %}
    <li class="lvl1"><a href="{{ routes.account_login_url }}">{{ 'customer.login.form_title' | t }}</a></li> 
    {% endif %}	  

    {%- when 'single_link' -%}
    <li class="lvl1" {{ block.shopify_attributes }}>{{linkLevel1 | replace: '<i class="at at-angle-down-l"></i>','' }}</li>

    {%- when 'megamenu1' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style1{% if block.settings.imgcall == "right" %} flex f-ais{% endif %}{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %}style="width:{{block.settings.mm_size}}px;"{% endif %}>
          {% if block.settings.box_sized %}<div class="mm_layout{% if block.settings.imgcall == "right" %} fl f-ais{% endif %}">{% endif %}
        	{%- capture imgColumns -%}
            {%- capture my_variable -%}{{block.settings.megamenu1_image}} {{block.settings.megamenu2_image}} {{block.settings.megamenu3_image}} {{block.settings.megamenu4_image}}{%- endcapture -%}
            {%- assign products_array = my_variable | split: ' ' -%}
            {%- if products_array.size > 0 -%}
                {%- if block.settings.megamenu1_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ block.settings.imageurl }}">
                        <img class="autoHt lazyload" data-src="{{ block.settings.megamenu1_image | img_url: 'master' }}" width="{{ block.settings.megamenu1_image.width }}" height="{{ block.settings.megamenu1_image.height }}" alt="{{block.settings.megamenu1_image.alt}}">
                        {% if block.settings.imageTtl1 != blank %}<span class="ttl">{{block.settings.imageTtl1}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if block.settings.megamenu2_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ block.settings.Image2url }}">
                        <img class="autoHt lazyload" data-src="{{ block.settings.megamenu2_image | img_url: 'master' }}" width="{{ block.settings.megamenu2_image.width }}" height="{{ block.settings.megamenu2_image.height }}" alt="{{block.settings.megamenu2_image.alt}}">
                      	{% if block.settings.imageTtl2 != blank %}<span class="ttl">{{block.settings.imageTtl2}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if block.settings.megamenu3_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ block.settings.Image3url }}">
                        <img class="autoHt lazyload" data-src="{{ block.settings.megamenu3_image | img_url: 'master' }}" width="{{ block.settings.megamenu3_image.width }}" height="{{ block.settings.megamenu3_image.height }}" alt="{{block.settings.megamenu3_image.alt}}">
                      	{% if block.settings.imageTtl3 != blank %}<span class="ttl">{{block.settings.imageTtl3}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if block.settings.megamenu4_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ block.settings.Image4url }}">
                        <img class="autoHt lazyload" data-src="{{ block.settings.megamenu4_image | img_url: 'master' }}" width="{{ block.settings.megamenu4_image.width }}" height="{{ block.settings.megamenu4_image.height }}" alt="{{block.settings.megamenu4_image.alt}}">
                        {% if block.settings.imageTtl4 != blank %}<span class="ttl">{{block.settings.imageTtl4}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
          	{%- endif -%}
          {%- endcapture -%}
          {% if block.settings.megamenu1_image != blank or block.settings.megamenu2_image != blank or block.settings.megamenu3_image != blank or block.settings.megamenu4_image != blank %}
       		<ul class="grid rwcols-{{block.settings.mmcolumns}}{% if block.settings.imgcall == "right" %} flex-item{%- endif -%}">{% for link in linklists[block.settings.mm_linklist].links %}{% render 'mm-links', link:link %}{% endfor %}</ul>
           	<ul class="grid f-ais rwcols-{{block.settings.mmimgcolumns}}" {% if block.settings.imgcall == "right" %}style="width:{{block.settings.columnWidth}}%;margin-left:15px;"{%- endif -%}>{{imgColumns}}</ul>
          {%- else -%}
        	<ul class="grid rwcols-{{block.settings.mmcolumns}} flex-item mmWrapper">{% for link in linklists[block.settings.mm_linklist].links %}{% render 'mm-links', link:link %}{% endfor %}</ul>
          {%- endif -%}
         {% if block.settings.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu2' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu grid--table style2{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %}style="width:{{block.settings.mm_size}}px;"{% endif %}>
       {% if block.settings.box_sized %}<div class=" grid--table mm_layout">{% endif %}
        <div class="gitem prColumn">
          <ul class="grid rwcols-{{block.settings.mmcolumns}}">
            {%- assign collection = collections[block.settings.collection] -%}
            {% for product in collection.products limit: block.settings.pr_limit %}
            <li class="gitem productCol">{% render 'mm-product', product:product %}</li>
            {% endfor %}
          </ul>
          {%- if block.settings.prodall != blank -%}<a href="{{collection.url}}" class="btn">{{block.settings.prodall}}</a>{%- endif -%}
        </div>
        {%- if block.settings.ftcl -%}
        <div class="gitem mmftcolumn tc" style="background-color:{{block.settings.ftcl_bg}};width:{{block.settings.ftcl_width}}%;">
          {%- if block.settings.ft_link != blank -%}<a href="{{block.settings.ft_link}}">{%- endif -%}
          {%- if block.settings.ftcl_image != blank -%}
          {%- assign mm2img_url = block.settings.ftcl_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          <p><img class="imgFl lazyload" src="{{ block.settings.ftcl_image | img_url: '50x' }}" width="{{ block.settings.ftcl_image.width }}" height="{{ block.settings.ftcl_image.height }}"
                  data-src="{{ mm2img_url }}" data-widths="[180, 360, 540, 720, 900]" data-aspectratio="{{ block.settings.ftcl_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.ftcl_image.alt | escape }}"></p>
          {%- endif -%}
          {%- if block.settings.ftcl_title != blank -%}<h4 style="color:{{block.settings.ftcl_txt}};font-size:{{block.settings.ftcl_title_size}}px;">{{block.settings.ftcl_title}}</h4>{%- endif -%}
          {%- if block.settings.ftcl_title != blank -%}<p style="color:{{block.settings.ftcl_txt}};font-size:{{block.settings.ftcl_subtitle_size}}px;">{{block.settings.ftcl_subtitle}}</p>{%- endif -%}
          {%- if block.settings.ftcl_title != blank -%}<span class="btn" style="color:{{block.settings.ftcl_txt}};background-color:{{block.settings.ftcl_bg}}; border:2px solid {{block.settings.ftcl_txt}};">{{block.settings.ftcl_btn}}</span>{%- endif -%}
          {%- if block.settings.ft_link != blank -%}</a>{%- endif -%}
        </div>
         {% if block.settings.box_sized %}</div>{% endif %}
      </div>
      {%- endif %}
    </li>
    {%- endif -%}

    {%- when 'megamenu3' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style3{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %}style="width:{{block.settings.mm_size}}px;"{% endif %}>
       {% if block.settings.box_sized %}<div class="mm_layout">{% endif %}
          {%- capture menucolumns -%}{{block.settings.ttl1 | remove: " "}} {{block.settings.ttl2 | remove: " "}} {{block.settings.ttl3 | remove: " "}} {{block.settings.ttl4 | remove: " "}} {{block.settings.ttl5 | remove: " "}}{%- endcapture -%}
          {%- assign columnitems = menucolumns | split: ' ' -%}
        <ul class="grid rwcols-{{columnitems.size}} mmWrapper">
          {%- assign mm3img = '380x' -%}
          {%- if columnitems.size == 4 -%}
              {% assign mm3img = '475x' %}
          {% elsif columnitems.size == 3 %}
              {% assign mm3img = '640x' %}
          {% elsif columnitems.size == 2 %}
              {% assign mm3img = '900x' %}
          {% elsif columnitems.size == 1 %}
              {% assign mm3img = '1900x' %}
          {%- endif -%}

          {%- if block.settings.ttl1 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label1 = block.settings.ttl1 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if block.settings.ttlurl1 != blank %}{{ block.settings.ttlurl1 }}{% else %}#{% endif %}" class="lvl-1">
              {%- assign img_url1 = block.settings.megamenu1_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          	  <img class="autoHt lazyload" width="{{ block.settings.megamenu1_image.width }}" height="{{ block.settings.megamenu1_image.height }}" data-src="{{ img_url1 }}" data-widths="[180, 360, 540, 720]"
                  data-aspectratio="{{ block.settings.megamenu1_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.megamenu1_image.alt | escape }}">
              <span class="title">{{ block.settings.ttl1 | split: '[label' | first }} {% if block.settings.ttl1 contains '[label' %}<span class="lbl {{m3label1}}">{{ section.settings[m3label1] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[block.settings.mm_linkcol1].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if block.settings.ttl2 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label2 = block.settings.ttl2 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if block.settings.ttlurl2 != blank %}{{ block.settings.ttlurl2 }}{% else %}#{% endif %}" class="lvl-1">
              {%- assign img_url2 = block.settings.megamenu2_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          	  <img class="autoHt lazyload" width="{{ block.settings.megamenu2_image.width }}" height="{{ block.settings.megamenu2_image.height }}" data-src="{{ img_url2 }}" data-widths="[180, 360, 540, 720]"
                  data-aspectratio="{{ block.settings.megamenu2_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.megamenu2_image.alt | escape }}">
              <span class="title">{{ block.settings.ttl2 | split: '[label' | first }} {% if block.settings.ttl2 contains '[label' %}<span class="lbl {{m3label2}}">{{ section.settings[m3label2] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[block.settings.mm_linkcol2].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if block.settings.ttl3 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label3 = block.settings.ttl3 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if block.settings.ttlurl3 != blank %}{{ block.settings.ttlurl3 }}{% else %}#{% endif %}" class="lvl-1">
              {%- assign img_url3 = block.settings.megamenu3_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          	  <img class="autoHt lazyload" width="{{ block.settings.megamenu3_image.width }}" height="{{ block.settings.megamenu3_image.height }}" data-src="{{ img_url3 }}" data-widths="[180, 360, 540, 720]"
                  data-aspectratio="{{ block.settings.megamenu3_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.megamenu3_image.alt | escape }}">
              <span class="title">{{ block.settings.ttl3 | split: '[label' | first }} {% if block.settings.ttl3 contains '[label' %}<span class="lbl {{m3label3}}">{{ section.settings[m3label3] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[block.settings.mm_linkcol3].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if block.settings.ttl4 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label4 = block.settings.ttl4 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if block.settings.ttlurl4 != blank %}{{ block.settings.ttlurl4 }}{% else %}#{% endif %}" class="lvl-1">
              {%- assign img_url4 = block.settings.megamenu4_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          	  <img class="autoHt lazyload" width="{{ block.settings.megamenu4_image.width }}" height="{{ block.settings.megamenu4_image.height }}" data-src="{{ img_url4 }}" data-widths="[180, 360, 540, 720]"
                  data-aspectratio="{{ block.settings.megamenu4_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.megamenu4_image.alt | escape }}">
              <span class="title">{{ block.settings.ttl4 | split: '[label' | first }} {% if block.settings.ttl4 contains '[label' %}<span class="lbl {{m3label4}}">{{ section.settings[m3label4] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[block.settings.mm_linkcol4].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if block.settings.ttl5 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label5 = block.settings.ttl5 |  split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if block.settings.ttlurl5 != blank %}{{ block.settings.ttlurl5 }}{% else %}#{% endif %}" class="lvl-1">
              {%- assign img_url5 = block.settings.megamenu5_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          	  <img class="autoHt lazyload" width="{{ block.settings.megamenu5_image.width }}" height="{{ block.settings.megamenu5_image.height }}" data-src="{{ img_url5 }}" data-widths="[180, 360, 540, 720]"
                  data-aspectratio="{{ block.settings.megamenu5_image.aspect_ratio }}" data-sizes="auto" alt="{{ block.settings.megamenu5_image.alt | escape }}">
              <span class="title">{{ block.settings.ttl5 | split: '[label' | first }} {% if block.settings.ttl5 contains '[label' %}<span class="lbl {{m3label5}}">{{ section.settings[m3label5] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[block.settings.mm_linkcol5].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
        </ul>
         {% if block.settings.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu4' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      {%- if linklists[block.settings.mm_linklist].links != blank -%}
      <div class="megamenu style4{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}" style="{% if block.settings.custom_size %}width:{{block.settings.mm_size}}px;{% endif %}{% if block.settings.mm4_bg != blank %}background-image:url({{block.settings.mm4_bg | img_url: "master"}});background-position:top {{block.settings.mm4_img_position}};{% endif %}">
         {% if block.settings.box_sized %}<div class="mm_layout">{% endif %}
          <ul class="grid rwcols-{{block.settings.mmcolumns}} mmWrapper" {% if block.settings.mm4_bg != blank %}style="padding-{{block.settings.mm4_img_position}}:{{block.settings.mm4_space}};"{% endif %}>
            {% for link in linklists[block.settings.mm_linklist].links %}{%- render 'mm-links', link:link -%}{% endfor %}
          </ul>
        {% if block.settings.box_sized %}</div>{% endif %}
      </div>
      {%- endif -%}
    </li>
    {%- endif -%}

    {%- when 'megamenu5' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style5{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %} style="width:{{block.settings.mm_size}}px;"{% endif %}>
        {% if block.settings.box_sized %}<div class="mm_layout">{% endif %}
        <ul class="grid rwcols-{{block.settings.mmcolumns}} mmWrapper {{block.settings.mm_linkStyle}}">
          {%- for link in linklists[block.settings.mm_linklist].links -%}
          <li class="gitem lvl-1">
            {%- assign navImg = link.title | handleize -%}
            <a href="{{ link.url }}" class="site-nav lvl-1">
              {%- if block.settings.mm_linkStyle == "above" -%}<span class="imgOver">{{ link.title }}</span>{%- endif -%}
              <img class="imgFl lazyload" data-src="{{ navImg | append: '.jpg' | file_img_url: '400x' }}" width="400" height="400" alt="{{ link.title }}" />
              {%- if block.settings.mm_linkStyle != "above" -%}<span class="imgOver">{{ link.title }}</span>{%- endif -%}
            </a>
          </li>
          {%- endfor -%}
        </ul>
        {% if block.settings.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu7' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style5{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %} style="width:{{block.settings.mm_size}}px;"{% endif %}>
        {% if block.settings.box_sized %}<div class="mm_layout">{% endif %}
        {{ block.settings.mm_html }}
        <div>&nbsp;</div>
        {% if block.settings.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}
  
  	{%- when 'megamenu6' -%}
    {%- if block.settings.title != blank -%}
    <li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style6{% if block.settings.imgcall == "right" %} flex{% endif %}{% if block.settings.custom_size %} {{block.settings.mm_align}}{% else %} mmfull{% endif %}"{% if block.settings.custom_size %}style="width:{{block.settings.mm_size}}px;"{% endif %}>
          {% if block.settings.box_sized %}<div class="mm_layout{% if block.settings.imgcall == "right" %} flex{% endif %}">{% endif %}
        	<ul class="fl w_100 mmWrapper">
              <li class="mm6main">
                  {% for link in linklists[block.settings.mm_linklist].links %}
                      {%- assign label1 = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                      <a href="{{ link.url }}" class="site-nav lvl-1{% if link.links != blank %} hasSub{% endif %}" data-link="#nv{{link.title | handleize}}">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label1] }}</span>{% endif %}</a>
                  {% endfor %}
              </li>
        	  <li class="lvl-1 fl1">
                  {% for link in linklists[block.settings.mm_linklist].links %}
                	{% assign nindex = forloop.index %}
                	<div id="nv{{link.title | handleize}}" class="fl admmsub{% if forloop.first %} active{% endif %}">
                      {%- if link.links != blank -%}
                        <ul class="subLinks fl1">
                          <li class="fl1 lvl-2{% if childlink.active or childlink.child_active %} active{% endif %}" style="column-count:{{block.settings.mmcolumns}};">
                          {%- for childlink in link.links -%}
                            {%- assign label2 = childlink.title |  split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
                            <a href="{{ childlink.url }}" class="site-nav lvl-2">{{ childlink.title | split: '[label' | first }}{% if childlink.title contains '[label' %}<span class="lbl {{label2}}">{{ section.settings[label2] }}</span>{% endif %}</a>
